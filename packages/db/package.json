{
  "name": "@teatime-ai/db",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./*": {
      "default": "./src/*.ts"
    },
    "./prisma/generated/client": {
      "types": "./prisma/generated/client.ts",
      "default": "./prisma/generated/client.ts"
    },
    "./prisma/generated/enums": {
      "types": "./prisma/generated/enums.ts",
      "default": "./prisma/generated/enums.ts"
    }
  },
  "scripts": {
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev"
  },
  "devDependencies": {
    "prisma": "^7.0.1",
    "@teatime-ai/config": "workspace:*"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@prisma/client": "catalog:",
    "@prisma/adapter-libsql": "^7.0.1",
    "dotenv": "catalog:",
    "zod": "^4.1.11"
  }
}
