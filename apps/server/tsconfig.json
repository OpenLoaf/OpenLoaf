{
	"extends": "@openloaf/config/tsconfig.base.json",
	"compilerOptions": {
		"composite": true,
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx"
	},
	"include": ["src/**/*.ts", "src/**/*.tsx"],
	"exclude": ["dist/**", "node_modules/**"]
}
