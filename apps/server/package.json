{
  "name": "server",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun --hot src/index.ts",
    "dev:inspect": "bun --inspect run --hot src/index.ts",
    "start": "bun run dist/index.js"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "^2.0.0-beta.47",
    "@ai-sdk/openai": "^3.0.0-beta.87",
    "@hono/trpc-server": "^0.4.0",
    "@teatime-ai/api": "workspace:*",
    "@teatime-ai/db": "workspace:*",
    "@trpc/server": "catalog:",
    "@types/uuid": "^11.0.0",
    "ai": "^6.0.0-beta.138",
    "dotenv": "catalog:",
    "hono": "catalog:",
    "uuid": "^13.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "typescript": "^5.8.2",
    "@types/bun": "^1.2.6",
    "tsdown": "^0.16.5",
    "@teatime-ai/config": "workspace:*"
  }
}
